	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.2.*",
		"platform/access": "2.0.*",
		"platform/content": "2.0.*",
		"platform/foundation": "2.0.*"
	},
	"require-dev": {
		"cartalyst/testing": "~1.0",
		"patchwork/utf8": "~1.1",
		"platform/menus": "1.0.*",
		"platform/users": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Platform\\Pages\\" : "src/"
		}
	},
	"extra": {
		"component": "extension",
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		}
	},
	"minimum-stability": "dev"
}
