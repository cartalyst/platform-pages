{
	"name": "platform/pages",
	"type": "platform-extension",
	"description": "An extension to manage your website pages.",
	"keywords": [
		"cartalyst",
		"platform",
		"laravel",
		"extension",
		"pages"
	],
	"license": "proprietary",
	"authors": [
