{
    "name": "platform/pages",
    "type": "platform-extension",
    "description": "A Platform extension to manage your website pages.",
    "keywords": [
        "cartalyst",
        "platform",
        "laravel",
        "extension",
        "pages"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        { "type": "composer", "url": "https://packages.cartalyst.com" }
    ],
    "require": {
        "php": ">=5.6.4",
        "cartalyst/composer-installers": "^2.0",
        "platform/access": "~6.0",
        "platform/content": "~6.0",
        "platform/foundation": "~6.0"
    },
    "require-dev": {
        "laravel/framework": "~5.3",
        "cartalyst/testing": "~2.0@dev",
        "patchwork/utf8": "~1.1",
        "platform/menus": "~6.0",
        "platform/users": "~6.0"
    },
    "autoload": {
        "classmap": [
            "resources/database/migrations",
            "resources/database/seeds"
        ],
        "psr-4": { "Platform\\Pages\\" : "src/" }
    },
    "extra": {
        "component": "extension",
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
