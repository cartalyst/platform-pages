{
    "name": "platform/pages",
    "type": "platform-extension",
    "description": "A Platform extension to manage your website pages.",
    "keywords": [
        "cartalyst",
        "platform",
        "laravel",
        "extension",
        "pages"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cartalyst.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "cartalyst/composer-installers": "^2.0",
        "platform/access": "~7.0",
        "platform/content": "~7.0",
        "platform/foundation": "~7.0"
    },
    "require-dev": {
        "cartalyst/php-cs-fixer-config": "^1.0",
        "laravel/framework": "~5.5",
        "cartalyst/testing": "~2.0@dev",
        "patchwork/utf8": "~1.1",
        "platform/menus": "~7.0",
        "platform/users": "~7.0"
    },
    "autoload": {
        "classmap": [
            "resources/database/migrations",
            "resources/database/seeds"
        ],
        "psr-4": {
            "Platform\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Platform\\Pages\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "component": "extension",
        "branch-alias": {
            "dev-master": "7.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
